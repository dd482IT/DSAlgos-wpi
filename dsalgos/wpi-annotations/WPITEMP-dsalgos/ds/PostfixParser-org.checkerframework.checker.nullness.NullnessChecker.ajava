package ds;

import java.util.Stack;

@org.checkerframework.framework.qual.AnnotatedFor("org.checkerframework.checker.nullness.NullnessChecker")
public class PostfixParser {

    private final @org.checkerframework.checker.initialization.qual.Initialized @org.checkerframework.checker.nullness.qual.NonNull Stack<Integer> theStack;

    private final @org.checkerframework.checker.initialization.qual.Initialized @org.checkerframework.checker.nullness.qual.NonNull String input;

    public PostfixParser(String s) {
        input = s;
        theStack = new Stack<>();
    }

    public  @org.checkerframework.checker.initialization.qual.Initialized @org.checkerframework.checker.nullness.qual.NonNull int parse() {
        int interAns = 0;
        for (int j = 0; j < input.length(); j++) {
            char ch = input.charAt(j);
            if (ch >= '0' && ch <= '9') {
                theStack.push((int) (ch - '0'));
                continue;
            }
            int num2 = theStack.pop();
            int num1 = theStack.pop();
            switch(ch) {
                case '+':
                    interAns = num1 + num2;
                    break;
                case '-':
                    interAns = num1 - num2;
                    break;
                case '*':
                    interAns = num1 * num2;
                    break;
                case '/':
                    interAns = num1 / num2;
                    break;
                case '%':
                    interAns = num1 % num2;
                    break;
                default:
                    interAns = 0;
                    break;
            }
            theStack.push(interAns);
        }
        if (!theStack.isEmpty())
            interAns = theStack.pop();
        return interAns;
    }
}
