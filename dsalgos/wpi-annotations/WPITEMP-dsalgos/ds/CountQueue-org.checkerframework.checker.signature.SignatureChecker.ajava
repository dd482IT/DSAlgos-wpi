    private final  @org.checkerframework.checker.signature.qual.SignatureUnknown int maxSize;

    private  @org.checkerframework.checker.signature.qual.SignatureUnknown long @org.checkerframework.checker.signature.qual.SignatureUnknown [] queArray;

    private  @org.checkerframework.checker.signature.qual.SignatureUnknown int front;

    private  @org.checkerframework.checker.signature.qual.SignatureUnknown int rear;

    private  @org.checkerframework.checker.signature.qual.SignatureUnknown int nItems;

    public CountQueue(int s) {
